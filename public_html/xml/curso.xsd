<?xml version="1.0" encoding="UTF-8"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified">
  <xs:element name="curso">
    <xs:complexType>
      <xs:sequence>
        <xs:element ref="estudiante"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="estudiante">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="codigo" type="xs:string"/>
        <xs:element name="nombre" type="xs:string"/>
        <xs:element ref="notas"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
  <xs:element name="notas">
    <xs:complexType>
      <xs:sequence>
        <xs:element name="nota" type="xs:string"/>
      </xs:sequence>
    </xs:complexType>
  </xs:element>
</xs:schema>
